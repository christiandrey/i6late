{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es5",
		"moduleResolution": "node",
		"module": "esnext",
		"noImplicitAny": false,
		"noEmitHelpers": true,
		"importHelpers": true,
		"esModuleInterop": true,
		"lib": ["es2017", "es5"],
		"outDir": "dist"
	},
	"allowSyntheticDefaultImports": true,
	"exclude": ["node_modules", "src/__tests__"]
}
